# coding=gbk
import os
from getpass import _raw_input
from _io import open


ls = os.linesep
fname=""
#get fileName
while True:
    if os.path.exists(fname):
        print("error:%s does not exists" % fname)
    else:
        break
    
all = []
print("\nEnter lines('.'by itself to quit).\n")

while True:
    entry = _raw_input(">")
    if entry=='.':
        break
    else:
        all.append(entry)
fobj = open(fname,"w")
fobj.writelines(['%s%s' % (x,ls) for x in all])
fobj.close()

